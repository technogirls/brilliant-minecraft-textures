{
    "multipart": [
        {   "apply": { "model": "block/fence/spruce_fence_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "block/fence/spruce_fence_side_ns", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "block/fence/spruce_fence_side_ns", "y": 90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "block/fence/spruce_fence_side_ew", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "block/fence/spruce_fence_side_ew", "y": 270, "uvlock": false }
        }
    ]
}
